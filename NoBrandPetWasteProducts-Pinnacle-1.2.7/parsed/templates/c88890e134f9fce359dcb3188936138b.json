  "components/common/subscription-popup-form": "<!-- {{#if theme_settings.newsletter_popup_title}}\n<h5 class=\"popup-info-heading\">{{theme_settings.newsletter_popup_title}}</h5>\n{{/if}} -->\n\n<!-- <h5 class=\"popup-info-heading\">{{lang 'common.newsletter_signup'}}</h5> -->\n\n{{#if theme_settings.newsletter_popup_description}}\n<p class=\"subscription-intro smaller lighter lowercase\">{{theme_settings.newsletter_popup_description}}</p>\n{{/if}}\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label u-hiddenVisually\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"Join\">\n                <div class=\"clear\"></div>\n\n            </div>\n            {{#if settings.show_newsletter_summary}}\n              <div class=\"popup-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n            <div class=\"clear\"></div>\n        </div>\n        <div class=\"clear\"></div>\n    </fieldset>\n</form>\n<div class=\"clear\"></div>\n"
}